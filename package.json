{
    "name": "back",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon src/app.ts",
        "build": "tsc",
        "start": "node dist/app.js",
        "typeorm": "typeorm",
        "migration:generate": "typeorm migration:generate -d ./src/data-source.ts",
        "migration:run": "typeorm migration:run -d ./src/data-source.ts",
        "docker:build": "docker-compose build",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:logs": "docker-compose logs -f",
        "prod:setup": "yarn docker:build && yarn docker:up",
        "prod:stop": "yarn docker:down",
        "prod:restart": "yarn docker:down && yarn docker:up",
        "prod:logs": "yarn docker:logs",
        "docker:migration:run": "docker-compose exec backend yarn migration:run",
        "deploy": "git pull && yarn prod:setup && yarn docker:migration:run"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/cors": "2.8.17",
        "@types/node": "20.10.0",
        "@types/express": "4.17.21",
        "cors": "2.8.5",
        "dotenv": "16.4.5",
        "express": "4.21.1",
        "mysql2": "3.11.5",
        "reflect-metadata": "0.2.2",
        "slugify": "1.6.6",
        "typeorm": "0.3.20"
    },
    "devDependencies": {
        "nodemon": "2.0.22",
        "ts-node-dev": "2.0.0",
        "typescript": "5.6.3"
    }
}
